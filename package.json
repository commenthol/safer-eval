{
  "name": "safer-eval",
  "version": "1.3.0",
  "description": "a safer eval",
  "keywords": [
    "eval",
    "safe"
  ],
  "bugs": {
    "url": "https://github.com/commenthol/safer-eval/issues"
  },
  "repository": {
    "type": "git",
